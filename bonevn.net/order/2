<script type="text/javascript">
$('#soluong').keyup(function()
{
    var soluong = $("#soluong").val();
    var ketqua = 150000 * soluong;
    var ketqua1 = soluong;


$('#ketqua').html(ketqua.toString().replace(/(.)(?=(\d{3})+$)/g,'$1,'));
$('#ketqua1').html(ketqua1.toString().replace(/(.)(?=(\d{3})+$)/g,'$1,'));
});
</script>



